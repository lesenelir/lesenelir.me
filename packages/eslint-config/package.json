{
  "name": "@lesenelir/eslint-config",
  "version": "0.0.0",
  "description": "lesenelir's ESLint config",
  "keywords": [
    "lesenelir",
    "eslint config"
  ],
  "homepage": "https://github.com/lesenelir/lesenelir.me/tree/master/packages/eslint-config#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lesenelir/lesenelir.me/tree/master/packages/eslint-config.git"
  },
  "license": "MIT",
  "author": "lesenelir <hi@lesenelir.me>",
  "type": "module",
  "exports": {
    ".": "./index.js",
    "./base": "./base.js",
    "./next": "./next.js",
    "./react-internal": "./react-internal.js"
  },
  "main": "index.js",
  "files": [
    "*.js"
  ],
  "scripts": {
    "commit-version": "git add . && git commit -m \"chore: release v$(pnpm run --silent get-version)\"",
    "get-version": "node -p \"require('./package.json').version\"",
    "release:major": "pnpm version major && pnpm run commit-version && pnpm publish",
    "release:minor": "pnpm version minor && pnpm run commit-version && pnpm publish",
    "release:patch": "pnpm version patch && pnpm run commit-version && pnpm publish"
  },
  "dependencies": {
    "@eslint/js": "^9.30.1",
    "@next/eslint-plugin-next": "^15.3.5",
    "@tanstack/eslint-plugin-query": "^5.81.2",
    "@typescript-eslint/parser": "^8.36.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.3.0",
    "typescript-eslint": "^8.36.0"
  },
  "peerDependencies": {
    "eslint": "^9.0.0",
    "typescript": "^5.0.0"
  },
  "packageManager": "pnpm@10.12.4"
}
